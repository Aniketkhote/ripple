

export component App() {
  <div>
    let obj = { $count: 0};

    <span>{"parent: "} {obj.$count}</span>
  </div>

  <div>
    let b = { $count: 0 };

    <button onClick={() => b.$count--}>{"-"}</button>
    <span>{b.$count}</span>
    <button onClick={() => b.$count++}>{"+"}</button>
  </div>
}

