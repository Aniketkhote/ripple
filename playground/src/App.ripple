import { effect } from 'ripple';

component Child(props) {
  
  effect(() => {
    console.log(props.$count);
  });

}

export component App() {
			const count = '$count';

			<div>
				let obj = {
					$count: 0
				};

				<span>{obj[count]}</span>
			</div>
			<div>
				let b = {
					$count: 0
				};

				<button onClick={() => b[count]--}>{'-'}</button>
				<span class='count'>{b[count]}</span>
				<button onClick={() => b[count]++}>{'+'}</button>
			</div>
}
